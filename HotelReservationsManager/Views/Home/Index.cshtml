@{
    ViewData["Title"] = "Home Page";
}

<style>
    body {
        background-color: #f7fbff; /* много светло синьо */
    }

    .card {
        background-color: #fffef0; /* много светло жълто */
        border: 1px solid #dee2e6;
        border-radius: 1rem;
        box-shadow: 0 4px 8px rgba(0, 123, 255, 0.1);
        transition: transform 0.3s ease;
    }

        .card:hover {
            transform: translateY(-5px);
            box-shadow: 0 8px 16px rgba(0, 123, 255, 0.15);
        }

    h1, h3 {
        color: #0056b3;
    }

    p {
        color: #4a4a4a;
    }

    .lead {
        color: #007bff;
    }

    .home-image {
        width: 50%; 
        height: auto;
        border-radius: 1rem;
        box-shadow: 0 4px 8px rgba(255, 193, 7, 0.2);
        margin-bottom: 2rem;
        display: block;
        margin-left: auto;
        margin-right: auto;
    }
</style>

<div class="container py-4">
    <div class="text-center mb-5">
        <h1 class="display-4">Welcome to Our Site</h1>
       
        
    </div>

    <div class="row mb-4">
        <div class="col-md-4 mb-3">
            <div class="card h-100">
                <div class="card-body text-center">
                    <h3>Easy to Use</h3>
                    <p>Our intuitive interface makes everything simple.</p>
                </div>
            </div>
        </div>
        <div class="col-md-4 mb-3">
            <div class="card h-100">
                <div class="card-body text-center">
                    <h3>Reliable</h3>
                    <p>Count on us whenever you need support.</p>
                </div>
            </div>
        </div>
        <div class="col-md-4 mb-3">
            <div class="card h-100">
                <div class="card-body text-center">
                    <h3>Powerful</h3>
                    <p>Advanced features with straightforward design.</p>
                </div>
            </div>
        </div>
        <img src="data:image/jpeg;base64,"
             alt="Welcome image"
             class="home-image" />
    </div>
</div>
